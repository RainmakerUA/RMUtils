		xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Button name="rmM2ButtonTemplate" parent="Minimap" virtual="true" inherit="SecureActionButtonTemplate"
			frameStrata="MEDIUM" frameLevel=8 hidden="true">
		<Size>
			<AbsDimension x="32" y="32"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="-60" y="-45"/>
				</Offset>
			</Anchor>
		</Anchors>
		<HitRectInsets>
			<AbsInset left="4" right="4" top="2" bottom="6"/>
		</HitRectInsets>
	</Button
</Ui>
